package com.sfx.factory;

public class DisplayFactory {

	public Display getDisplay(String displayType) {
		Display display;
		switch (displayType.toUpperCase()) {
		case "HD": {
			display = new DisplayHD();
		}
		case "4K": {
			display = new Display4k();
		}
		case "8KVR": {
			display = new Display8kVR();
		}
		default: {
			display = null;
		}
			return display;
		}
	}

}
