package com.sfx.factory;

import com.sfx.component.controller.Controller;
import com.sfx.component.display.Display;
import com.sfx.component.processor.Processor;

public class AverageGamerBundle implements ConsoleGamingFactory {

	@Override
	public Processor createProcessor() {
		return new ProcessorFactory().getProcessor("basic");
	}

	@Override
	public Display createDisplay() {
		return new DisplayFactory().getDisplay("hd");
	}

	@Override
	public Controller createController() {
		// TODO Auto-generated method stub
		return null;
	}

}
