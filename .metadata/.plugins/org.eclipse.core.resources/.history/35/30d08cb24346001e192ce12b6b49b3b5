package com.sfx.mp;

import java.util.Optional;

public class MaintenanceProcedureFactory {

	public static Optional<Object> getMaintanenceProcedure(String maintenanceProcedure) {
		Optional<Object> obj;
		switch (maintenanceProcedure) {
		case "engineClean": {
			obj = Optional.ofNullable(new EngineCleanProcudure());
			break;
		}
		case "oilChange": {
			obj = Optional.ofNullable(new OilChangeProcedure());
			break;
		}
		default: {
			obj = Optional.empty();
		}
		}
		return obj;
	}

}
