package com.sfx.factory;

import com.sfx.builder.GamingConsoleBuilder;
import com.sfx.pojo.GamingConsole;

public class BeginnerBundleFactory implements GamingConsoleBundleFactory {

	@Override
	public GamingConsole getInstance() {
		GamingConsoleBuilder.builder().controller("STANDARD").processor("BASIC").display("HD").build();
	}
	
}
