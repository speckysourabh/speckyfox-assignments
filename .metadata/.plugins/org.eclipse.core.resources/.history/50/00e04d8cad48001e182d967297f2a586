package com.sfx.component.display;

public class DisplayHD implements Display {

	@Override
	public void displayType() {
		System.out.println("HD display");
	}

	@Override
	public void price() {
		System.out.println(2000);
	}
	
	@Override
	public String displayType() {
		return "HD display";
	}

	@Override
	public int price() {
		return 2000;
	}

}
