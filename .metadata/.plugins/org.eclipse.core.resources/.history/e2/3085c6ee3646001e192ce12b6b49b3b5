package com.sfx.vehicle;

import java.util.Optional;

public class VehicleFactory {
	
	public static Vehicle getVehicle(String vehicle) {
		Optional<Object> obj;
		switch(vehicle) {
		case "bike" : {
				obj = Optional.ofNullable(new Bike());
				break;
		}
		
		case "truck" : {
			obj = Optional.ofNullable(new Truck());
			break;
		}
		
		default : {
			obj = Optional.empty();
		}
			
		}
		return obj;
	}
	
}
