package com.sfx.component.processor;

import com.sfx.component.processor.Processor;
import com.sfx.component.processor.Processor4k;
import com.sfx.component.processor.Processor8kVR;
import com.sfx.component.processor.ProcessorHD;

public class ProcessorFactory {

	public Processor getProcessor(String processorType) {
		Processor processor;
		switch (processorType.toUpperCase()) {
		case "HD": {
			processor = new ProcessorHD();
		}
		case "4K": {
			processor = new Processor4k();
		}
		case "8KVR": {
			processor = new Processor8kVR();
		}
		default: {
			processor = null;
		}
			return processor;
		}
	}
	
}
