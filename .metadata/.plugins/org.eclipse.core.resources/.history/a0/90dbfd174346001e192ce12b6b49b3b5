package com.sfx.mp;

import java.util.Optional;

public class MaintenanceProcedureFactory {
	
	public static MaintenanceProcedure getMaintanenceProcedure(String maintenanceProcedure) {
		MaintenanceProcedure obj;
		switch(maintenanceProcedure) {
		case "engineClean": {
			obj = new EngineCleanProcudure();
			break;
		}
		case "oilChange": {
			obj = new OilChangeProcedure();
			break;
		}
		default: {
			obj = Optional.empty()
		}
		}
 	}
	
}
