package com.sfx.pojo;

public class Customer {
	private String name;
	private Long mobNumber;
	private String email;
	private String type;
	public Customer(String name, Long mobNumber, String email, String type) {
		super();
		this.name = name;
		this.mobNumber = mobNumber;
		this.email = email;
		this.type = type;
	}
}
