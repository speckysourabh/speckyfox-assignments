package com.sfx.component.display;

public class Display8kVR implements Display {

	@Override
	public void displayType() {
		System.out.println("HD display");
	}

	@Override
	public void price() {
		System.out.println(8000);
	}
	
	@Override
	public String displayType() {
		return "8kVR display";
	}

	@Override
	public int price() {
		return 40000;
	}

}
